{
  "name": "@lsk4/tsup-config",
  "version": "4.1.0",
  "description": "TSUP configs for LSK.js projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "     ycmd dev",
    "build": "   ycmd build",
    "test": "    ycmd test",
    "prepack": " ycmd prepack",
    "release": " ycmd release"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./cjs/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./lib/*.d.ts",
      "default": "./cjs/*.js"
    }
  },
  "files": [
    "lib",
    "cjs",
    "README.md",
    "LICENCE"
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-presets/tsconifg",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs-presets/tsconifg",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "directory": ".release"
  },
  "keywords": [
    "eslint",
    "config",
    "lskjs"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
