{
  "name": "@lsk4/tsconfig",
  "version": "4.0.3",
  "description": "TS configs for LSK.js projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "     ycmd dev",
    "build": "   ycmd build",
    "test": "    ycmd test",
    "prepack": " ycmd prepack",
    "release": " ycmd release"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "main": "strict/tsconfig.json",
  "exports": {
    ".": {
      "import": "./strict/tsconfig.json",
      "require": "./strict/tsconfig.json",
      "node": "./strict/tsconfig.json",
      "default": "./strict/tsconfig.json"
    },
    "./strict": {
      "import": "./strict/tsconfig.json",
      "require": "./strict/tsconfig.json",
      "node": "./strict/tsconfig.json",
      "default": "./strict/tsconfig.json"
    },
    "./nostrict": {
      "import": "./nostrict/tsconfig.json",
      "require": "./nostrict/tsconfig.json",
      "node": "./nostrict/tsconfig.json",
      "default": "./nostrict/tsconfig.json"
    }
  },
  "files": [
    "base",
    "strict",
    "nostrict"
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-presets/tsconifg",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs-presets/tsconifg",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "config",
    "lskjs"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}