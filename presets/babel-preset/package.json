{
  "name": "@lsk4/babel-preset",
  "version": "4.1.0",
  "description": "Babel preset for LSK.js projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "contributors": [
    "Andrey Kondalov <andrey@kondalov.com> (https://github.com/yukioru)"
  ],
  "private": false,
  "scripts": {
    "dev": "     ycmd dev",
    "build": "   ycmd build",
    "test": "    ycmd test",
    "prepack": " ycmd prepack",
    "release": " ycmd release"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.20.7",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-proposal-function-bind": "7.18.9",
    "@babel/plugin-proposal-optional-chaining": "7.20.7",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.20.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.20.11",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@emotion/babel-plugin": "11.10.5"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/runtime": "7.20.7"
  },
  "peerDependencies": {
    "@babel/core": "7.x",
    "@babel/runtime": "7.x"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "main": "index.js",
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-presets/babel-preset",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs-presets/babel-preset",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "directory": ".release"
  },
  "keywords": [
    "eslint",
    "config",
    "lskjs"
  ],
  "files": [
    "index.js",
    "README.md"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
