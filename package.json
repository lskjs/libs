{
  "name": "@macrobe/libs2",
  "version": "1.2.1",
  "description": "Libs starter kit",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "type": "module",
  "scripts": {
    "release": "        set -a && source .env && set +a && multi-semantic-release --no-ci",
    "version": "        pnpm -r --workspace-concurrency=1 exec -- npx --no-install standard-version",
    "start": "          lsk run start",
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "release2": "        lsk run release",
    "clean": "          rm -rf libs/*/.release libs/*/.reports libs/*/coverage",
    "release3": "pnpm -r --workspace-concurrency=1 exec -- npx --no-install semantic-release -e semantic-release-monorepo",
    "standard-version": "standard-version",
    "semantic-release": "semantic-release --no-ci"
  },
  "devDependencies": {
    "@anolilab/multi-semantic-release": "^1.0.2",
    "@lskjs/cli-scripts": "^3.19.0",
    "@lskjs/eslint-config": "^3.18.0",
    "@lskjs/jest-config": "^3.18.0",
    "@lskjs/tsconfig": "^3.18.0",
    "@swc/core": "^1.3.96",
    "c8": "^8.0.1",
    "lsk": "^3.19.0",
    "semantic-release": "^22.0.7",
    "semantic-release-monorepo": "^7.0.5",
    "semantic-release-pnpm": "^1.0.2",
    "standard-version": "^9.5.0",
    "tsm": "^2.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "main": "index.js",
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/libs",
  "homepage": "https://github.com/lskjs/libs",
  "bugs": "https://github.com/lskjs/libs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "multi-release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator"
    ],
    "noCi": true,
    "remove": [
      "semantic-release-pnpm-esm"
    ]
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
