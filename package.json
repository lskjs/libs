{
  "name": "@macrobe/libss123",
  "version": "1.6.0",
  "description": "Libs starter kit",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "type": "module",
  "scripts": {
    "version": "        lerna version",
    "release": "        lerna version",
    "start": "          lsk run start",
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "clean": "          rm -rf libs/*/.release libs/*/.reports libs/*/coverage",
    "":"",
    "release:afterf": " git commit -am 'chore(release): publish v3.19.0' && git push && git push --tags",
    "version2": "        pnpm -r --workspace-concurrency=1 exec -- npx --no-install standard-version",
    "release2": "        lsk run release",
    "release3": "pnpm -r --workspace-concurrency=1 exec -- npx --no-install semantic-release -e semantic-release-monorepo",
    "standard-version": "standard-version",
    "release4": "        set -a && source .env && set +a && multi-semantic-release --no-ci --debug --deps.bump satisfy --deps.release patch",
    "semantic-release": "semantic-release --no-ci"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@lskjs/cli-scripts": "^3.19.0",
    "@lskjs/eslint-config": "^3.18.0",
    "@lskjs/jest-config": "^3.18.0",
    "@lskjs/tsconfig": "^3.18.0",
    "@release-it-plugins/workspaces": "^4.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@swc/core": "^1.3.96",
    "c8": "^8.0.1",
    "lerna": "^7.4.2",
    "lsk": "^3.19.0",
    "multi-semantic-release": "^3.0.2",
    "release-it": "^16.0.0",
    "semantic-release": "^22.0.7",
    "semantic-release-monorepo": "^7.0.5",
    "semantic-release-pnpm": "^1.0.2",
    "standard-version": "^9.5.0",
    "tsm": "^2.3.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "main": "index.js",
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/libs",
  "homepage": "https://github.com/lskjs/libs",
  "bugs": "https://github.com/lskjs/libs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "multi-release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo v${nextRelease.version}"
        }
      ]
    ],
    "noCi": true,
    "remove": [
      "semantic-release-pnpm-esm"
    ]
  },
  "workspaces": [
    "libs/algos",
    "libs/err"
  ],
  "release-it": {
    "git": {
      "commitMessage": "chore: ðŸŽ‰ release v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it-plugins/workspaces": true
    }
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
